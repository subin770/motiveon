<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="logImpl" value="LOG4J2"/>
    </settings>
	
    <typeAliases>
        <!-- 기본 자료형 -->
        <typeAlias type="java.lang.String" alias="str"/>
        <typeAlias type="java.lang.Integer" alias="int"/>

        <!-- VO Alias -->
        <typeAlias type="com.motiveon.dto.EmployeeVO" alias="employee"/>
        <typeAlias type="com.motiveon.dto.MenuVO" alias="menu"/>
        <typeAlias type="com.motiveon.dto.CalendarVO" alias="calendar"/>
        <typeAlias type="com.motiveon.dto.DepartmentVO" alias="department"/>
		<typeAlias type="com.motiveon.dto.AttachmentVO" alias="attachment"/>
        <typeAlias type="com.motiveon.dto.PopUpVO" alias="popup"/>
        <typeAlias type="com.motiveon.dto.NoticeVO" alias="notice" />
        <typeAlias type="com.motiveon.command.PageMaker" alias="pageMaker"/>

        <!-- 업무 -->
        <typeAlias type="com.motiveon.dto.WorkVO" alias="work"/>
        <typeAlias type="com.motiveon.dto.WorkManagerVO" alias="workManager"/>
        <typeAlias type="com.motiveon.dto.WorkAttachVO" alias="workAttach"/>
        <typeAlias type="com.motiveon.dto.WorkReplyVO" alias="workReply"/>

        
        <typeAlias type="com.motiveon.dto.PostPositionVO" alias="postposition"/>
		<typeAlias type="com.motiveon.dto.HrVO" alias="hr"/>
        
    </typeAliases>

    
</configuration>
	